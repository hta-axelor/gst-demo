<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">

	<menuitem name="gst-root" title="GST" />

	<menuitem title="Company" name="Company" parent="gst-root" />
	<menuitem title="Party" name="Party" parent="gst-root" />
	<menuitem title="Product" name="Product" parent="gst-root" />



	<menuitem name="gst-root-Company" parent="Company" title="Company"
		action="action.gst.view.company.company" />

	<action-view name="action.gst.view.company.company" title="Company"
		model="com.axelor.gst.db.Company">
		<view type="grid" name="company-grid" />
		<view type="form" name="company-form" />
	</action-view>

	<menuitem name="gst-root-BankAccount" parent="Company" title="BankAccount"
		action="action.gst.view.company.bankaccount" />

	<action-view name="action.gst.view.company.bankaccount" title="Bank Account"
		model="com.axelor.gst.db.BankAccount">
		<view type="grid" name="bankaccount-grid" />
		<view type="form" name="bankaccount-form" />
	</action-view>
	
	
	
	
	
	<menuitem name="gst-root-Product" parent="Product" title="Product"
		action="action.gst.view.product.product" />

	<action-view name="action.gst.view.product.product" title="Product"
		model="com.axelor.gst.db.Product">
		<view type="grid" name="product-grid" />
		<view type="form" name="product-form" />
	</action-view>
	
	<menuitem name="gst-root-ProductCategory" parent="Product" title="Product Categories"
		action="action.gst.view.product.productcategory" />

	<action-view name="action.gst.view.product.productcategory" title="Product Categories"
		model="com.axelor.gst.db.ProductCategory">
		<view type="grid" name="productcategory-grid" />
		<view type="form" name="productcategory-form" />
	</action-view>






	<menuitem name="gst-root-Party-Party" parent="Party" title="Party"
		action="action.gst.view.party.party" />

	<action-view name="action.gst.view.party.party" title="Party"
		model="com.axelor.gst.db.Party">
		<view type="grid" name="party-grid" />
		<view type="form" name="party-form" />
	</action-view>

	<menuitem name="gst-root-Party-Contact" parent="Party" title="Contact"
		action="action.gst.view.party.contact" />

	<action-view name="action.gst.view.party.contact" title="Contact"
		model="com.axelor.gst.db.Contact">
		<view type="grid" name="contact-grid" />
		<view type="form" name="contact-form" />
	</action-view>



	<menuitem title="Configurations" name="Configurations" parent="Party" icon="fa-cog"/>



	<menuitem name="gst-root-Party-Configurations-Address" parent="Configurations"
		title="Address" action="action.gst.view.party.configurations.address" />

	<action-view name="action.gst.view.party.configurations.address" title="Address"
		model="com.axelor.gst.db.Address">
		<view type="grid" name="address-grid" />
		<view type="form" name="address-form" />
	</action-view>
	
	<menuitem name="gst-root-Party-Configurations-City" parent="Configurations"
		title="City" action="action.gst.view.party.configurations.city" />

	<action-view name="action.gst.view.party.configurations.city" title="City"
		model="com.axelor.gst.db.City">
		<view type="grid" name="city-grid" />
		<view type="form" name="city-form" />
	</action-view>
	
	<menuitem name="gst-root-Party-Configurations-State" parent="Configurations"
		title="State" action="action.gst.view.party.configurations.state" />

	<action-view name="action.gst.view.party.configurations.state" title="State"
		model="com.axelor.gst.db.State">
		<view type="grid" name="state-grid" />
		<view type="form" name="state-form" />
	</action-view>
	
	<menuitem name="gst-root-Party-Configurations-Country" parent="Configurations"
		title="Country" action="action.gst.view.party.configurations.country" />

	<action-view name="action.gst.view.party.configurations.country" title="Country"
		model="com.axelor.gst.db.Country">
		<view type="grid" name="country-grid" />
		<view type="form" name="country-form" />
	</action-view>

</object-views>