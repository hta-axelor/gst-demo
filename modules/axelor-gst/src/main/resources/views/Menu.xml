<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">

	<menuitem name="gst-root" title="GST" />


	<!-- Company -->
	<menuitem name="gst-root-company" title="Company" parent="gst-root" />

	<menuitem name="gst-root-company-company" title="Company"
		parent="gst-root-company" action="gst.root.company.company" />

	<action-view name="gst.root.company.company" title="Company"
		model="com.axelor.gst.db.Company">
		<view type="grid" name="company-grid" />
		<view type="form" name="company-form" />
	</action-view>

	<menuitem name="gst-root-company-bank-account" title="Bank Account"
		parent="gst-root-company" action="gst.root.company.bank.account" />

	<action-view name="gst.root.company.bank.account" title="Bank Account"
		model="com.axelor.gst.db.BankAccount">
		<view type="grid" name="bank-account-grid" />
		<view type="form" name="bank-account-form" />
	</action-view>
	
	<menuitem name="gst-root-company-sequence" title="Sequence"
		parent="gst-root-company" action="gst.root.company.sequence" />

	<action-view name="gst.root.company.sequence" title="Sequence"
		model="com.axelor.gst.db.Sequence">
		<view type="grid" name="sequence-grid" />
		<view type="form" name="sequence-form" />
	</action-view>




	<!-- Party -->
	<menuitem name="gst-root-party" title="Party" parent="gst-root" />

	<menuitem name="gst-root-party-party" title="Party" parent="gst-root-party"
		action="gst.root.party.party" />

	<action-view name="gst.root.party.party" title="Party"
		model="com.axelor.gst.db.Party">
		<view type="cards" name="party-cards" />
		<view type="grid" name="party-grid" />
		<view type="form" name="party-form" />
		<context name="json-enhance" expr="true" />
	</action-view>

	<menuitem name="gst-root-party-contact" title="Contact"
		parent="gst-root-party" action="gst.root.party.contact" />

	<action-view name="gst.root.party.contact" title="Contact"
		model="com.axelor.gst.db.Contact">
		<view type="grid" name="contact-grid" />
		<view type="form" name="contact-form" />
	</action-view>

	<menuitem name="gst-root-party-configurations" title="Configurations"
		parent="gst-root-party" icon="fa-cog" />

	<menuitem name="gst-root-party-configurations-address" title="Address"
		parent="gst-root-party-configurations" action="gst.root.party.configurations.address" />

	<action-view name="gst.root.party.configurations.address"
		title="Address" model="com.axelor.gst.db.Address">
		<view type="grid" name="address-grid" />
		<view type="form" name="address-form" />
	</action-view>

	<menuitem name="gst-root-party-configurations-city" title="City"
		parent="gst-root-party-configurations" action="gst.root.party.configurations.city" />

	<action-view name="gst.root.party.configurations.city"
		title="City" model="com.axelor.gst.db.City">
		<view type="grid" name="city-grid" />
		<view type="form" name="city-form" />
	</action-view>

	<menuitem name="gst-root-party-configurations-state" title="State"
		parent="gst-root-party-configurations" action="gst.root.party.configurations.state" />

	<action-view name="gst.root.party.configurations.state"
		title="State" model="com.axelor.gst.db.State">
		<view type="grid" name="state-grid" />
		<view type="form" name="state-form" />
	</action-view>

	<menuitem name="gst-root-party-configurations-country" title="Country"
		parent="gst-root-party-configurations" action="gst.root.party.configurations.country" />

	<action-view name="gst.root.party.configurations.country"
		title="Country" model="com.axelor.gst.db.Country">
		<view type="grid" name="country-grid" />
		<view type="form" name="country-form" />
	</action-view>




	<!-- Product -->
	<menuitem name="gst-root-product" title="Product" parent="gst-root" />


	<menuitem name="gst-root-product-product" title="Product"
		parent="gst-root-product" action="gst.root.product.product" />

	<action-view name="gst.root.product.product" title="Product"
		model="com.axelor.gst.db.Product">
		<view type="cards" name="product-cards" />
		<view type="grid" name="product-grid" />
		<view type="form" name="product-form" />
		<context name="json-enhance" expr="true" />
	</action-view>

	<menuitem name="gst-root-product-product-category" title="Product Categories"
		parent="gst-root-product" action="gst.root.product.product.category" />

	<action-view name="gst.root.product.product.category"
		title="Product Categories" model="com.axelor.gst.db.ProductCategory">
		<view type="grid" name="product-category-grid" />
		<view type="form" name="product-category-form" />
	</action-view>




	<!-- Invoice -->
	<menuitem name="gst-root-invoice" title="Invoice" parent="gst-root" />

	<menuitem name="gst-root-invoice-create-invoice" title="Create Invoice"
		parent="gst-root-invoice" action="gst.root.invoice.create.invoice"
		order="1" />

	<action-view name="gst.root.invoice.create.invoice" title="Create Invoice"
		model="com.axelor.gst.db.Invoice">
		<view type="form" name="invoice-form" />
	</action-view>

	<menuitem name="gst-root-invoice-draft-invoices" title="Draft Invoices"
		parent="gst-root-invoice" action="gst.root.invoice.draft.invoices"
		order="2" />

	<action-view name="gst.root.invoice.draft.invoices" title="Draft Invoices"
		model="com.axelor.gst.db.Invoice">
		<view type="grid" name="invoice-grid" />
		<view type="form" name="invoice-form" />
		<view type="kanban" name="invoice-kanban" />
		<domain>self.statusSelect = '1'</domain>
	</action-view>

	<menuitem name="gst-root-invoice-validated-invoices" title="Validated Invoices"
		parent="gst-root-invoice" action="gst.root.invoice.validated.invoices"
		order="3" />

	<action-view name="gst.root.invoice.validated.invoices"
		title="Validated Invoices" model="com.axelor.gst.db.Invoice">
		<view type="grid" name="invoice-grid" />
		<view type="form" name="invoice-form" />
		<domain>self.statusSelect = '2'</domain>
	</action-view>

	<menuitem name="gst-root-invoice-paid-invoices" title="Paid Invoices"
		parent="gst-root-invoice" action="gst.root.invoice.paid.invoices"
		order="4" />

	<action-view name="gst.root.invoice.paid.invoices" title="Paid Invoices"
		model="com.axelor.gst.db.Invoice">
		<view type="grid" name="invoice-grid" />
		<view type="form" name="invoice-form" />
		<domain>self.statusSelect = '3'</domain>
	</action-view>

	<menuitem name="gst-root-invoice-cancelled-invoices" title="Cancelled Invoices"
		parent="gst-root-invoice" action="gst.root.invoice.cancelled.invoices"
		order="5" />

	<action-view name="gst.root.invoice.cancelled.invoices"
		title="Cancelled Invoices" model="com.axelor.gst.db.Invoice">
		<view type="grid" name="invoice-grid" />
		<view type="form" name="invoice-form" />
		<domain>self.statusSelect = '4'</domain>
	</action-view>

	<menuitem name="gst-root-invoice-all-invoices" title="All Invoices"
		parent="gst-root-invoice" action="gst.root.invoice.all.invoices"
		order="5" />

	<action-view name="gst.root.invoice.all.invoices"
		title="All Invoices" model="com.axelor.gst.db.Invoice">
		<view type="grid" name="invoice-grid" />
		<view type="form" name="invoice-form" />
	</action-view>


</object-views>