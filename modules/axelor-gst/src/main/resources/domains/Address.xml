<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

  <module name="gst" package="com.axelor.gst.db"/>

  <entity name="Address"> 
    <string name="type" selection="gst.address.type.selection" default="default"/>
    <string name="line1"/>
    <string name="line2"/>
    <many-to-one name="city" ref="City"/>
    <many-to-one name="state" ref="State"/>
    <many-to-one name="country" ref="Country"/>
   ,<string name="pincode"/>
  </entity>

</domain-models>
