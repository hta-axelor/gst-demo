<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

  <module name="gst" package="com.axelor.gst.db"/>

  <entity name="Contact"> 
    <string name="name"/>
    <string name="type" selection="gst.contact.type.selection"/>
    <string name="primaryEmail"/>
    <string name="secondaryEmail"/>
    <string name="primaryPhone"/>
    <string name="secondaryPhone"/>
  </entity>

</domain-models>
