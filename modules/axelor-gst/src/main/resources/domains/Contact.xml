<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="gst" package="com.axelor.gst.db"/>

  <entity name="Contact"> 
    <string name="name" title="Name" required="true"/>
    <string name="type" title="Type" selection="gst.contact.type.selection" required="true" />
    <string name="primaryEmail" title="Primary Email"/>
    <string name="secondaryEmail" title="Secondary Email"/>
    <string name="primaryPhone" title="Primary Phone"/>
    <string name="secondaryPhone" title="Secondary Phone"/>
    
    <extra-code><![CDATA[
		  public static final String CONTACT_PRIMARY = "1";
		  public static final String CONTACT_SECONDARY = "2";
		  public static final String CONTACT_OTHERS = "3";
		]]></extra-code>
    
  </entity>

</domain-models>
