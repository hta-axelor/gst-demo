<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="gst" package="com.axelor.gst.db" />

	<entity name="Sequence">
       <string name="prefix" title="Prefix" required="true"/>
       <string name="suffix" title="Suffix" />
       <integer name="padding" title="Padding" min="1" max="10" default="1"/>
       <string name="nextNumber" title="Next Number" readonly="true"/>
       <many-to-one name="metaModel" title="Model" ref="com.axelor.meta.db.MetaModel" required="true"/>
	</entity>

</domain-models>
